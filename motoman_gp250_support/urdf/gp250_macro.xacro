<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:macro name="motoman_gp250" params="prefix robot_tf_frame_prefix:=r1/">
    <xacro:include filename="$(find motoman_resources)/urdf/common_materials.xacro"/>

    <!-- link list -->
    <link name="${prefix}base_link">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp250_support/meshes/gp250/visual/base_link_visual.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp250_support/meshes/gp250/collision/base_link_collision.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_1">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp250_support/meshes/gp250/visual/link_1_s_visual.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp250_support/meshes/gp250/collision/link_1_s_collision.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_2">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp250_support/meshes/gp250/visual/link_2_l_visual.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp250_support/meshes/gp250/collision/link_2_l_no_balancer_collision.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_3">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp250_support/meshes/gp250/visual/link_3_u_visual.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp250_support/meshes/gp250/collision/link_3_u_collision.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_4">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp250_support/meshes/gp250/visual/link_4_r_visual.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp250_support/meshes/gp250/collision/link_4_r_collision.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_5">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp250_support/meshes/gp250/visual/link_5_b_visual.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp250_support/meshes/gp250/collision/link_5_b_collision.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_6">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp250_support/meshes/gp250/visual/link_6_t_visual.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp250_support/meshes/gp250/collision/link_6_t_collision.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}tool0"/>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_s" type="revolute">
        <parent link="${prefix}base_link"/>
        <child link="${prefix}link_1"/>
        <origin xyz="0 0 0.650" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="${radians(-180)}" upper="${radians(180)}" effort="5516" velocity="${radians(180)}"/>
    </joint>
    <joint name="${prefix}joint_l" type="revolute">
        <parent link="${prefix}link_1"/>
        <child link="${prefix}link_2"/>
        <origin xyz="0.285 0 0" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <limit lower="${radians(-60)}" upper="${radians(76)}" effort="7060" velocity="${radians(178)}"/>
    </joint>
    <joint name="${prefix}joint_u" type="revolute">
        <parent link="${prefix}link_2"/>
        <child link="${prefix}link_3"/>
        <origin xyz="0 0 1.150" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="${radians(-77.8)}" upper="${radians(197)}" effort="3089" velocity="${radians(178)}"/>
    </joint>
    <joint name="${prefix}joint_r" type="revolute">
        <parent link="${prefix}link_3"/>
        <child link="${prefix}link_4"/>
        <origin xyz="0 0 0.250" rpy="0 0 0" />
        <axis xyz="-1 0 0" />
        <limit lower="${radians(-360)}" upper="${radians(360)}" effort="926" velocity="${radians(250)}"/>
    </joint>
    <joint name="${prefix}joint_b" type="revolute">
        <parent link="${prefix}link_4"/>
        <child link="${prefix}link_5"/>
        <origin xyz="1.285 0 0" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="${radians(-125)}" upper="${radians(125)}" effort="794" velocity="${radians(250)}"/>
    </joint>
    <joint name="${prefix}joint_t" type="revolute">
        <parent link="${prefix}link_5"/>
        <child link="${prefix}link_6"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="-1 0 0" />
        <limit lower="${radians(-360)}" upper="${radians(360)}" effort="375" velocity="${radians(360)}"/>
    </joint>
    <joint name="${prefix}joint_t-tool0" type="fixed">
        <origin xyz="0.250 0 0" rpy="3.1415926 -1.570796 0"/>
        <parent link="${prefix}link_6"/>
        <child link="${prefix}tool0"/>
    </joint>
    <!-- end of joint list -->

    <link name="${prefix}flange" />
    <joint name="${prefix}joint_t-flange" type="fixed">
        <origin xyz="0 0 0" rpy="0 ${radians(90)} 0" />
        <parent link="${prefix}link_6" />
        <child link="${prefix}flange" />
    </joint>


    <!-- ROS base_link to Robot Manufacturer World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
        <origin xyz="0 0 0.650" rpy="0 0 0"/>
        <parent link="${prefix}base_link"/>
        <child link="${prefix}base"/>
    </joint>

    <!-- Motoman world frame published from the robot is the same as the base -->
    <link name="${robot_tf_frame_prefix}world" />
    <joint name="${prefix}base_${robot_tf_frame_prefix}world" type="fixed">
        <origin xyz="0 0 0.0" rpy="0 0 0"/>
        <parent link="${prefix}base"/>
        <child link="${robot_tf_frame_prefix}world"/>
    </joint>

  </xacro:macro>
</robot>
